<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="用户管理系统测试套件">
    <test name="用户管理系统测试用例">
        <classes>
            <class name = "com.cases.LoginTest">
                <methods>
                    <include name="loginTrue" />
                    <include name="loginFalse" />
                </methods>
            </class>
            <class name = "com.cases.AddUserTest">
                <methods>
                    <include name="addUser" />
                </methods>
            </class>
            <class name = "com.cases.GetUserInfoListTest">
                <methods>
                    <include name="getUserListInfo" />
                </methods>
            </class>
            <class name = "com.cases.GetUserInfoTest">
                <methods>
                    <include name="getUserInfo" />
                </methods>
            </class>
            <class name = "com.cases.UpdateUserInfoTest">
                <methods>
                    <include name="updateUserInfo" />
                    <include name="deleteUser" />
                </methods>
            </class>
        </classes>
    </test>
    <listeners>
        <listener class-name="com.config.ExtentTestNGIReporterListener" />
    </listeners>
</suite>